<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dash_nav_graph.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.acedropseller.view.dash.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_authActivity"
            app:destination="@id/authActivity"
            app:popUpTo="@id/authActivity"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.acedropseller.view.dash.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <activity
        android:id="@+id/authActivity"
        android:name="com.example.acedropseller.view.auth.AuthActivity"
        android:label="activity_auth"
        tools:layout="@layout/activity_auth" />
    <fragment
        android:id="@+id/itemsFragment"
        android:name="com.example.acedropseller.view.dash.ItemsFragment"
        android:label="fragment_items"
        tools:layout="@layout/fragment_items" >
        <action
            android:id="@+id/action_itemsFragment_to_uploadProductFragment"
            app:destination="@id/uploadProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/uploadProductFragment"
        android:name="com.example.acedropseller.view.dash.UploadProductFragmentsp"
        android:label="fragment_upload_product"
        tools:layout="@layout/fragment_upload_product" />
</navigation>